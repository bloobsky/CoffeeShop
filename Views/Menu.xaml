<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CoffeeShop.Views.Menu"
             xmlns:viewModels="clr-namespace:CoffeeShop.ViewModels"
             xmlns:components="clr-namespace:CoffeeShop.Components"
             xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
             x:DataType="viewModels:MenuViewModel"
             Title="Menu">
    <ContentPage.Behaviors>
        <toolkit:EventToCommandBehavior EventName="Loaded" Command="{Binding DataCommand}"/>
    </ContentPage.Behaviors>
    <components:MenuListComponent/>
</ContentPage>